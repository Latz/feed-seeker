{
	"name": "feedseeker",
	"version": "1.0.0",
	"description": "FeedSeeker - Find RSS, Atom, and JSON feeds on any website with support for preserving query parameters",
	"main": "dist/feed-seeker.cjs",
	"module": "dist/feed-seeker.js",
	"type": "module",
	"bin": {
		"feed-seeker": "dist/feed-seeker-cli.cjs"
	},
	"exports": {
		".": {
			"import": "./dist/feed-seeker.js",
			"require": "./dist/feed-seeker.cjs"
		},
		"./cli": {
			"import": "./dist/feed-seeker-cli.js",
			"require": "./dist/feed-seeker-cli.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "node tests/run-tests.js",
		"test:unit": "node --test tests/*.test.js",
		"build": "vite build"
	},
	"keywords": [
		"rss",
		"atom",
		"feed",
		"finder",
		"rss-finder",
		"feed-discovery",
		"web-feed",
		"syndication",
		"query-parameters"
	],
	"author": "Your Name <your.email@example.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/feedseeker.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/feedseeker/issues"
	},
	"homepage": "https://github.com/yourusername/feedseeker#readme",
	"dependencies": {
		"async": "^3.2.6",
		"chalk": "^5.6.2",
		"commander": "^14.0.1",
		"linkedom": "^0.18.7",
		"parse5": "^7.2.1",
		"tldts": "^7.0.16",
		"truncate-url": "^3.0.0"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"devDependencies": {
		"vite": "^7.1.7"
	}
}
