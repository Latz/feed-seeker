{
	"name": "feed-seeker",
	"version": "1.0.0",
	"description": "Feed Seeker - Find RSS, Atom, and JSON feeds on any website with support for preserving query parameters",
	"main": "dist/feed-seeker.cjs",
	"module": "dist/feed-seeker.js",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/feed-seeker.js",
			"require": "./dist/feed-seeker.cjs"
		},
		"./metaLinks": {
			"import": "./dist/modules/metaLinks.js",
			"require": "./dist/modules/metaLinks.cjs"
		},
		"./anchors": {
			"import": "./dist/modules/anchors.js",
			"require": "./dist/modules/anchors.cjs"
		},
		"./blindsearch": {
			"import": "./dist/modules/blindsearch.js",
			"require": "./dist/modules/blindsearch.cjs"
		},
		"./deepSearch": {
			"import": "./dist/modules/deepSearch.js",
			"require": "./dist/modules/deepSearch.cjs"
		},
		"./eventEmitter": {
			"import": "./dist/modules/eventEmitter.js",
			"require": "./dist/modules/eventEmitter.cjs"
		},
		"./fetchWithTimeout": {
			"import": "./dist/modules/fetchWithTimeout.js",
			"require": "./dist/modules/fetchWithTimeout.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "node --import tsx tests/run-tests.js",
		"test:unit": "node --import tsx --test tests/*.test.js",
		"test:watch": "node --import tsx --test --watch tests/*.test.js",
		"test:coverage": "c8 --reporter=text --reporter=html --reporter=lcov node --import tsx --test tests/*.test.js",
		"test:coverage:report": "c8 report --reporter=text",
		"build": "tsc && vite build",
		"type-check": "tsc --noEmit",
		"prebuild": "npm run type-check",
		"dev": "tsc --watch",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build && npm test"
	},
	"keywords": [
		"rss",
		"atom",
		"feed",
		"finder",
		"rss-finder",
		"feed-discovery",
		"web-feed",
		"syndication",
		"query-parameters"
	],
	"author": "Your Name <your.email@example.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/feed-seeker.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/feed-seeker/issues"
	},
	"homepage": "https://github.com/yourusername/feed-seeker#readme",
	"dependencies": {
		"async": "^3.2.6",
		"chalk": "^5.6.2",
		"commander": "^14.0.1",
		"eta": "^4.0.1",
		"isurl-module": "^0.10.0",
		"linkedom": "^0.18.7",
		"normalize-url": "^8.0.1",
		"ora": "^9.0.0",
		"parse5": "^7.2.1",
		"progress-estimator": "^0.3.1",
		"tldts": "^7.0.16",
		"truncate-url": "^3.0.0"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"devDependencies": {
		"@types/async": "^3.2.25",
		"@types/node": "^24.10.0",
		"c8": "^10.1.3",
		"terser": "^5.44.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vite": "^7.1.7",
		"vite-plugin-dts": "^4.5.4"
	}
}
